{
  "name": "ergonomic-theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class": {
      "fontStyle": "bold",
      "foreground": "#4488FF"
    },
    "type": {
      "fontStyle": "bold",
      "foreground": "#4488FF"
    },
    "interface": {
      "fontStyle": "bold",
      "foreground": "#4488FF"
    },
    "enum": {
      "fontStyle": "bold",
      "foreground": "#4488FF"
    },
    "enumMember": "#ffff44",
    "typeParameter": {
      "fontStyle": "bold",
      "foreground": "#4488FF"
    },
    "namespace": {
      "fontStyle": "bold",
      "foreground": "#4488FF"
    },
    "function": {
      "fontStyle": "bold",
      "foreground": "#44ff44"
    },
    "method": {
      "fontStyle": "bold",
      "foreground": "#44ff44"
    },
    "variable": "#ffff44",
    "parameter": "#ff8844",
    "property": {
      "foreground": "#ff4488"
    },
    "comment": "#008800",
    "string": "#aaffaa",
    "operator": "#FFFFFF",
    "keyword": "#FF44FF",
    "stringLiteral": "#aaffaa",
    "struct": {
      "fontStyle": "bold",
      "foreground": "#4488FF"
    },
    "macro": {
      "fontStyle": "italic bold",
      "foreground": "#44ff44"
    },
    "newOperator": "#FF44FF",
    "numberLiteral": "#aaffff",
    "number": "#aaffff",
    "customLiteral": "#ff0000",
    "decorator": "#ff0000",
    "event": "#ff0000",
    "genericType": "#ff0000",
    "label": "#ff0000",
    "memberOperatorOverload": "#ff0000",
    "operatorOverload": "#ff0000",
    "referenceType": "#ff0000",
    "regexp": "#ff0000",
    "templateFunction": "#ff0000",
    "templateType": "#ff0000",
    "valueType": "#ff0000",
    "cliProperty": "#ff0000",
    "*.defaultLibrary": {
      "fontStyle": "italic bold"
    },
    "variable.defaultLibrary": {
      "fontStyle": "italic bold",
      "foreground": "#44ffff"
    },
    "*.global": {
      "fontStyle": "italic bold"
    },
    "*.static": {
      "fontStyle": "italic bold"
    },
    "*.modification": {},
    "*.readonly": {},
    "*.abstract": {},
    "*.async": {},
    "*.declaration": {},
    "*.deprecated": {},
    "*.documentation": {},
    "*.local": {}
  },
  "tokenColors": [
    {
      "scope": [
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#aaffaa"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#aaffff"
      }
    },
    {
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#aaaaff"
      }
    },
    {
      "name": "this",
      "scope": [
        "variable.language.this"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#ff4488"
      }
    },
    {
      "name": "value: null, undefined",
      "scope": [
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#ffaaff"
      }
    },
    {
      "name": "xxx.xxxx.XXXX",
      "scope": [
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#ff4488"
      }
    },
    {
      "name": "json key",
      "scope": [
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#ff4488"
      }
    },
    {
      "name": "interface, type, const, class, constructor, let",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#FF44FF"
      }
    },
    {
      "name": "private, public, readonly, extends",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FF44FF"
      }
    },
    {
      "name": "export, if, return, new, :, =, +, |, <,, ++,",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#FF44FF"
      }
    },
    {
      "name": "new, :, =, +, |, <,, ++,",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "new, :, =, +, |, <,, ++,",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression"
      ],
      "settings": {
        "foreground": "#FF44FF"
      }
    },
    {
      "name": "number, string, boolean, any, unknown",
      "scope": [
        "support.type.primitive"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#44ffff"
      }
    },
    {
      "name": "undefined, null",
      "scope": [
        "support.type.builtin"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#44ffff"
      }
    },
    {
      "name": "{, }, ;, ${}, `, \", ', :, .",
      "scope": [
        "punctuation.definition",
        "punctuation.section",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "`, \", '",
      "scope": [
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#aaffaa"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#008800"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#44ff44"
      }
    },
    {
      "name": "jsx string",
      "scope": [
        "meta.jsx.children"
      ],
      "settings": {
        "foreground": "#aaffaa"
      }
    },
    {
      "name": "jsx tag",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#44ffff"
      }
    },
    {
      "name": "jsx class tag",
      "scope": [
        "support.class.component"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4488FF"
      }
    },
    {
      "name": "jsx class tag param",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#ff4488"
      }
    },
    {
      "scope": [
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#4488FF"
      }
    },
    {
      "scope": [
        "markup.inline.raw.string"
      ],
      "settings": {
        "foreground": "#aaffaa"
      }
    }
  ]
}