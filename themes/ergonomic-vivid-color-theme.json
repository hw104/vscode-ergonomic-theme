{
  "name": "ergonomic-vivid",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class": "#4488FF",
    "type": "#4488FF",
    "interface": "#4488FF",
    "enum": "#4488FF",
    "enumMember": "#ffff44",
    "typeParameter": "#4488FF",
    "namespace": "#4488FF",
    "struct": "#4488FF",
    "function": {
      "italic": true,
      "foreground": "#44ff44"
    },
    "method": {
      "italic": true,
      "foreground": "#44ff44"
    },
    "macro": {
      "italic": true,
      "foreground": "#44ff44"
    },
    "variable": "#ffff44",
    "parameter": "#ff8844",
    "property": "#ff4488",
    "property.annotation": "#FF44FF",
    "parameter.label": "#E0E0E0",
    "comment": "#008800",
    "string": "#aaffaa",
    "operator": "#FFFFFF",
